{
  parser: "babel-eslint",
  "rules": {
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "indent": [1, 4],
    "react/jsx-indent": [1, 4],
    "linebreak-style": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,

  },
  "env": {
    "browser": true,
    "node": true,
  },
  "extends": ["airbnb"],
}
